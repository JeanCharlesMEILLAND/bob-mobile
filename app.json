{
  "expo": {
    "name": "bob-mobile",
    "slug": "bob-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "webpack",
      "build": {
        "babel": {
          "include": [
            "react-native-vector-icons",
            "react-native-paper"
          ]
        }
      }
    },
    "plugins": [
      [
        "expo-contacts",
        {
          "contactsPermission": "Bob a besoin d'accéder à vos contacts pour vous aider à retrouver vos amis qui utilisent déjà l'application et inviter les autres à vous rejoindre."
        }
      ]
    ]
  }
}